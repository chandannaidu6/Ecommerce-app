{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "@repo/ui": ["packages/ui"],
        "@repo/utils": ["packages/utils"]
      }
    }
  }
  